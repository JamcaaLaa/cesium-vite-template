{
  "name": "cesium-vite-template",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@6.28.0^",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "postinstall": "node static-copy.js",
    "static-copy": "node static-copy.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "cesium": "^1.91.0",
    "del": "^6.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "recursive-copy": "^2.0.14",
    "typescript": "^4.6.2",
    "vite": "^2.6.0",
    "vite-plugin-externals": "^0.4.0",
    "vite-plugin-html-config": "^1.0.8"
  },
  "lint-staged": {
    "vite.config.ts": [
      "prettier --write"
    ],
    "gulpfile.{js,ts}": [
      "prettier --write"
    ],
    "src/**/*.{ts,js,json,css}": [
      "prettier --write"
    ]
  }
}
